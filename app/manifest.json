{
  "content_security_policy": "script-src 'self' https://globo.com/ 'unsafe-eval'; object-src 'self'","name": "Symplometro",
  "short_name": "Symplometro",
  "description": "Easy way to get how many events exist!",
  "version": "0.0.2",
  "manifest_version": 2,
  "icons": {
    "16": "images/symplometro-20.png",
    "128": "images/symplometro-128.png"
  },
  "background": {
    "scripts": ["jquery.min.js", "moment.js", "background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_icon": {
      "19": "images/symplometro-20.png",
      "38": "images/symplometro-20.png"
    },
    "default_title": "Symplometro",
    "default_popup": "popup.html"
  },
  "author": "Glauro Quint√£o Juliani",
  "content_scripts": [
    {
      "js": [
        "/jquery.min.js",
        "/vuex.js",
        "/vue.js",
        "/moment.js"
      ],
      "css": [
        "base.css"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "cookies",
    "alarms",
    "notifications",
    "background",
    "webRequest",
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [
    "bootstrap.min.css"
  ]
}
