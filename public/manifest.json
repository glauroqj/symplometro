{
  "name": "Symplometro",
  "short_name": "Symplometro",
  "description": "Contador de eventos. Apenas para symplers! =]",
  "version": "0.1.1",
  "manifest_version": 2,
  "author": "Glauro Quint√£o Juliani",
  "icons": {
    "16": "symplometro-20.png",
    "128": "symplometro-128.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_icon": {
      "19": "symplometro-20.png",
      "38": "symplometro-20.png"
    },
    "default_title": "Symplometro",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "cookies",
    "alarms",
    "notifications",
    "background",
    "webRequest",
    "http://*/*",
    "https://*/*"
  ]
}
