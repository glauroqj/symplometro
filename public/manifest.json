{
  "name": "Symplometro",
  "short_name": "Symplometro",
  "description": "Contador de eventos. Apenas para symplers! =]",
  "version": "0.1.3",
  "manifest_version": 2,
  "author": "Glauro Quint√£o Juliani",
  "content_security_policy":"script-src 'self' https://www.gstatic.com/ https://*.firebaseio.com https://www.googleapis.com; object-src 'self'",
  "icons": {
    "16": "symplometro-20.png",
    "128": "symplometro-128.png"
  },
  "background": {
    "page": "bg.html",
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "19": "symplometro-20.png",
      "38": "symplometro-20.png"
    },
    "default_title": "Symplometro",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "cookies",
    "notifications",
    "background",
    "http://*/*",
    "https://*/*"
  ]
}
